<select id="getIndexData"  resultMap="streetdto">

select a.*, b.lng,b.lat,city.city_name from street a LEFT JOIN areas b on a.street_name_id = b.name_id
LEFT join  city  on a.city_id = city.city_name_id
where b.areas_status =2 and  a.create_date> '2020-04-01 17:00:56' and city.create_date>'2020-04-01 17:00:56'

</select>



